{
  "name": "@sip-protocol/sdk",
  "version": "0.7.3",
  "description": "Core SDK for Shielded Intents Protocol - Privacy layer for cross-chain transactions",
  "author": "SIP Protocol <hello@sip-protocol.org>",
  "homepage": "https://sip-protocol.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/sip-protocol/sip-protocol.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/sip-protocol/sip-protocol/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.js"
    },
    "./proofs/noir": {
      "types": "./dist/proofs/noir.d.ts",
      "import": "./dist/proofs/noir.mjs",
      "require": "./dist/proofs/noir.js"
    },
    "./proofs/halo2": {
      "types": "./dist/proofs/halo2.d.ts",
      "import": "./dist/proofs/halo2.mjs",
      "require": "./dist/proofs/halo2.js"
    },
    "./proofs/kimchi": {
      "types": "./dist/proofs/kimchi.d.ts",
      "import": "./dist/proofs/kimchi.mjs",
      "require": "./dist/proofs/kimchi.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/browser.ts src/proofs/noir.ts src/proofs/halo2.ts src/proofs/kimchi.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/browser.ts src/proofs/noir.ts src/proofs/halo2.ts src/proofs/kimchi.ts --format cjs,esm --dts --watch",
    "lint": "eslint --ext .ts src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench --config vitest.bench.config.ts",
    "bench:json": "vitest bench --config vitest.bench.config.ts --outputJson benchmarks/results.json",
    "example:auction": "tsx examples/auction-example.ts"
  },
  "optionalDependencies": {
    "https-proxy-agent": "^7.0.0",
    "socks-proxy-agent": "^8.0.0"
  },
  "dependencies": {
    "@aztec/bb.js": "3.0.2",
    "@ethereumjs/rlp": "^10.1.0",
    "@jup-ag/api": "^6.0.48",
    "@langchain/core": "^0.3.30",
    "@langchain/openai": "^0.4.2",
    "@magicblock-labs/ephemeral-rollups-sdk": "^0.8.0",
    "@noble/ciphers": "^2.1.1",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@noir-lang/noir_js": "1.0.0-beta.18",
    "@noir-lang/types": "1.0.0-beta.18",
    "@radr/shadowwire": "^1.1.2",
    "@scure/base": "^2.0.0",
    "@scure/bip32": "^2.0.1",
    "@scure/bip39": "^2.0.1",
    "@sip-protocol/types": "^0.2.1",
    "@solana-program/compute-budget": "^0.11.0",
    "@solana-program/system": "^0.10.0",
    "@solana/compat": "^5.4.0",
    "@solana/kit": "^5.4.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@triton-one/yellowstone-grpc": "^4.0.2",
    "langchain": "^1.2.10",
    "pino": "^10.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "1.6.1",
    "fast-check": "^4.5.3",
    "https-proxy-agent": "^7.0.0",
    "socks-proxy-agent": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "sip",
    "privacy",
    "intents",
    "cross-chain",
    "stealth-addresses",
    "zcash"
  ],
  "license": "MIT"
}
