{
  "name": "@sip-protocol/sdk",
  "version": "0.2.10",
  "description": "Core SDK for Shielded Intents Protocol - Privacy layer for cross-chain transactions",
  "author": "SIP Protocol <hello@sip-protocol.org>",
  "homepage": "https://sip-protocol.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/sip-protocol/sip-protocol.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/sip-protocol/sip-protocol/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.js"
    },
    "./proofs/noir": {
      "types": "./dist/proofs/noir.d.ts",
      "import": "./dist/proofs/noir.mjs",
      "require": "./dist/proofs/noir.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/browser.ts src/proofs/noir.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/browser.ts src/proofs/noir.ts --format cjs,esm --dts --watch",
    "lint": "eslint --ext .ts src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench --config vitest.bench.config.ts",
    "bench:json": "vitest bench --config vitest.bench.config.ts --outputJson benchmarks/results.json"
  },
  "dependencies": {
    "@aztec/bb.js": "^0.63.1",
    "@noble/ciphers": "^2.0.1",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@noir-lang/noir_js": "^1.0.0-beta.16",
    "@noir-lang/types": "1.0.0-beta.16",
    "@sip-protocol/types": "^0.1.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "1.6.1",
    "fast-check": "^4.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "sip",
    "privacy",
    "intents",
    "cross-chain",
    "stealth-addresses",
    "zcash"
  ],
  "license": "MIT"
}
