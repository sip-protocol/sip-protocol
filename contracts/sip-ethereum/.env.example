# SIP Ethereum Contract Deployment Configuration
# Copy this to .env and fill in your values
# NEVER commit .env to version control!

# DEPLOYMENT KEYS (REQUIRED)
PRIVATE_KEY=0x...

# CONTRACT CONFIGURATION (optional, defaults to deployer)
# OWNER_ADDRESS=0x...
# FEE_COLLECTOR=0x...
# FEE_BPS=100

# ETHEREUM MAINNET / SEPOLIA
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_KEY
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_KEY
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_KEY

# BASE (60%+ L2 tx share - CRITICAL)
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
BASESCAN_API_KEY=YOUR_BASESCAN_KEY

# ARBITRUM (44% TVL - CRITICAL)
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc
ARBISCAN_API_KEY=YOUR_ARBISCAN_KEY

# OPTIMISM (6% TVL - HIGH PRIORITY)
OPTIMISM_RPC_URL=https://mainnet.optimism.io
OPTIMISM_SEPOLIA_RPC_URL=https://sepolia.optimism.io
OPTIMISM_ETHERSCAN_API_KEY=YOUR_OPTIMISM_KEY

# UPGRADE CONFIGURATION (only needed for upgrades)
# SIP_PRIVACY_ADDRESS=0x...
